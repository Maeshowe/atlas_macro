# ATLAS MACRO - Environment Configuration
# Copy to .env and fill in your API keys

# API Keys (required for LIVE mode)
POLYGON_KEY=your_polygon_api_key
FRED_KEY=your_fred_api_key

# Operating Mode: LIVE (fetch from APIs) or READ_ONLY (cache only)
OPERATING_MODE=LIVE

# Cache TTL in days
CACHE_TTL_DAYS=7

# Logging
LOG_LEVEL=INFO
